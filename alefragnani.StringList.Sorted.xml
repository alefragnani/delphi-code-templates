<?xml version="1.0" encoding="utf-8" ?>
<codetemplate	xmlns="http://schemas.borland.com/Delphi/2005/codetemplates"
				version="1.0.0">
	<template name="slsorted" surround="true" invoke="manual">
		<description>
			StringList Sorted
		</description>
		<author>
			Alessandro Fragnani
		</author>
    <point name="Object">      
			<text>Object</text>
			<hint>StringList Object</hint>
		</point>
    <point name="duplicates">     
      <script language="Delphi">
				InvokeCodeCompletion;
			</script> 
			<text>dupIgnore</text>
			<hint>Duplicate Config</hint>
		</point>
		<code language="Delphi" delimiter="|"><![CDATA[|Object|.Duplicates := [|duplicates|];
|Object|.Sorted := True;|*||end|]]>
		</code>
	</template>
</codetemplate>