<?xml version="1.0" encoding="utf-8" ?>
<codetemplate	xmlns="http://schemas.borland.com/Delphi/2005/codetemplates"
				version="1.0.0">
	<template name="DUnitInit" invoke="manual">
		<description>
			DUnit Initialization
		</description>
